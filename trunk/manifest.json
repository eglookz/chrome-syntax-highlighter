{
  "version": "1.0",
  "name": "SynHighlighter",
  "description": "Syntax highlighter for source code files",
  "icons": { 
	"48": "res/icon48.png",
    "128": "res/icon128.png" 
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*", "ftp://*/*", "file://*/*"],
      "js": ["core/jquery-1.3.2.min.js", "core/syntaxhighlighter/shCore.js", 
		"core/syntaxhighlighter/brushes/shBrushCSharp.js", "core/syntaxhighlighter/brushes/shBrushDelphi.js", "core/syntaxhighlighter/brushes/shBrushJava.js", 
		"core/syntaxhighlighter/brushes/shBrushXml.js", "core/syntaxhighlighter/brushes/shBrushJScript.js",
		"core/core.js",
		"content-script.js"]
    }
  ]
}